{
    "parser": "babel-eslint",
    "extends": [
        "prettier",
        "airbnb"
    ],
    "plugins": [
        "jest"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "jest/globals": true
    },
    "globals": {
        "API_URL": true,
        "token": true
    },
    "rules": {
        "arrow-parens": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "react/require-default-props": 0
    }
}